page {
  background: #f5f5f5;
  position: absolute;
  /* overflow-y: hidden; */
}
.top {
  width: 100%;
  background: #ffe600;
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  justify-content: space-between;
  padding: 0 4%;
  box-sizing: border-box;
  z-index: 10;
}
.top{
  height: 140rpx;
  line-height: 200rpx;
}
.ios{
  height: 180rpx;
  line-height: 260rpx;
}
.top view {
  width: 33.3%;
}

.top view:nth-of-type(1) image {
  width: 33rpx;
  height: 33rpx;
}

.top view:nth-of-type(2) {
  text-align: center;
  font-size: 32rpx;
  color: #060606;
}
.list_ul{
  width: 100%;
  margin-top: 140rpx;
  overflow: hidden;
}
.listxia{
  margin-top: 180rpx;
}
.list_ul .ul_li{
  width: 100%;
  height: 160rpx;
  margin-top: 20rpx;
  background: white;
  display: flex;
  justify-content: space-between;
  padding: 0 4%;
  box-sizing: border-box;
  align-content: center;
}
.list_ul .ul_li .left{
  width: 80%;
  height: 100%;
  display: flex;
  align-items: center;
}
.list_ul .ul_li .left image{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}
.list_ul .ul_li .left text{
  font-size: 34rpx;
  color: #060606;
  margin-left: 30rpx;
}
.list_ul .ul_li .right{
  display: flex;
  align-items: center;
}
.list_ul .ul_li .right view{
  width: 139rpx;
  height: 45rpx;
  text-align: center;
  border: 1px solid #979797;
  border-radius: 23.5px;
  font-size: 26rpx;
  color: #464646;
  line-height: 45rpx;
}
.kong{
  width: 364rpx;
  height: 344rpx;
  display: block;
  margin: 257rpx auto;
}
